      <codeSegments>
        <codeSegment>
          <version>4.0.0/4.0.99</version>
          <code>
            <![CDATA[

    volatile Map<String, Plugin> pluginMap;

    protected void cleanDelegateCache() {
        this.flushPluginMap();
    }

    /**
     * Reset the {@link pluginMap} field to {@code null}
     */
    public void flushPluginMap() {
        this.pluginMap = null;
    }

    /**
     * @return a Map of plugins field with {@link Plugin#getKey()} as key
     * @see Plugin#getKey()
     */
    public Map<String, Plugin> getPluginsAsMap() {
        if (pluginMap == null) {
            synchronized (this) {
                Object[] pluginArray = this.getPlugins().toArray();
                Map.Entry<String, ? extends Plugin>[] entries = new Map.Entry[pluginArray.length];
                int i = 0;
                for (Plugin pluginObject : pluginArray) {
                    Plugin plugin = (Plugin) pluginObject;
                    entries[i] = Map.entry(
                            plugin.getKey(),
                            plugin
                    );
                    i+=1;
                }
                pluginMap = Map.ofEntries(
                    entries
                );
            }
        }
        return pluginMap;
    }
            ]]>
          </code>
